        "compile:enums": "npm run compile:paths && npm run compile:score-key",
        "compile:paths": "tsc src/models/paths.ts && mv src/models/paths.js src/models/webpack-paths.js",
        "compile:score-key": "tsc src/models/score-key.ts && mv src/models/score-key.js src/models/webpack-score-key.js",
        "compile:ts": "tsc --noEmit -p tsconfig.json",
        "start": "node index.js",
        "start:dev": "webpack serve --config webpack/development.config.js"
    },
